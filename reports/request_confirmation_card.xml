<?xml version="1.0" encoding="utf-8"?>
<odoo>
<template id="report_request">
    <t t-call="web.html_container">
        <t t-foreach="docs" t-as="o">
            <t t-call="web.external_layout">
                <div class="page">
                    <h2>Cash Request</h2>
                    <p>This object's name is <span t-field="o.title"/></p>
                </div>
            </t>
        </t>
    </t>
</template>
</odoo>